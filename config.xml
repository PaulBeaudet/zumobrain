<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>zumoBrain</name>
    <description>
        Brain for the Polulu Zumo robot
    </description>
    <author email="inofor@gmail.com" href="branded.me/paulbeaudet">
        Paul Beaudet
    </author>
    <content src="bot.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="fr.drangies.cordova.serial" spec="https://github.com/xseignard/cordovarduino.git" />
    <engine name="android" spec="~5.1.1" />
    <engine name="browser" spec="~4.0.0" />
</widget>
